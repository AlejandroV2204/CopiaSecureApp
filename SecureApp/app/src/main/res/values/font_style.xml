<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="txt_textoTitulos">
        <item name="android:fontFamily">@font/librebaskerville_bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/Texto_color</item>
    </style>
    <style name="txt_textoNormal">
        <item name="android:fontFamily">@font/librebaskerville_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/Texto_color</item>
    </style>
    <style name="txt_textoAlertas">
        <item name="android:fontFamily">@font/librebaskerville_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/Texto_color</item>
        <item name="android:textAlignment">center</item>
    </style>

    <style name="img_imageRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    style="@style/txt_textoTitulos"
    style="@style/txt_textoNormal"
</resources>